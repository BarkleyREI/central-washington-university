<section class="iframe component">

	{{#if title }}<h2 class="iframe__title font-h2">{{{ title }}}</h2><hr />{{/if}}
	{{#if copy }}<p class="iframe__copy font-body">{{{ copy }}}</p>{{/if}}

	{{#with iframe}}<iframe src="{{{ src }}}" height="{{ height }}" width="{{ width }}" title="{{ title }}"></iframe>{{/with }}
	
</section>